{
  "name": "root",
  "private": true,
  "devDependencies": {
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.{js,css,md,ts,jsx,tsx}": "prettier --write"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "lerna bootstrap",
    "start": "lerna run --parallel start",
    "start:dev": "lerna run --parallel start:dev",
    "test": "lerna run --parallel test",
    "generate": "npx openapi-typescript http://localhost:4200/api-json --output packages/frontend/src/typing.ts"
  },
  "dependencies": {
    "openapi-typescript": "^5.2.0",
    "openapi-typescript-fetch": "^1.1.2"
  }
}
